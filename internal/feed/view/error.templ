package view

import (
	"fmt"
	"github.com/tjanas94/vibefeeder/internal/feed/models"
)

// Error renders the feed list error component (debug version)
templ Error(vm models.FeedListErrorViewModel) {
	<div class="p-4">
		<h1 class="text-2xl font-bold mb-4">internal/feed/view/error.templ</h1>
		<pre>{ fmt.Sprintf("%+v", vm) }</pre>
	</div>
}
