# Supabase Configuration (Required)
SUPABASE_URL=
SUPABASE_KEY=

# E2E User Configuration (Required)
# E2E_USERNAME_ID: UUID of the test user, used for cleanup and test data isolation
E2E_USERNAME_ID=
E2E_USERNAME=
E2E_PASSWORD=

# GitHub Access Token (Optional)
# Personal Access Token for accessing GitHub-hosted test feed fixtures
# Used to avoid rate limiting on GitHub raw content URLs
# Permissions needed: Public repositories (read-only) or no special scopes required
# The token will be embedded in feed URLs as: https://TOKEN@raw.githubusercontent.com/...
GITHUB_ACCESS_TOKEN=
